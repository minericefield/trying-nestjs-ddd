{
  "name": "trying-nestjs-ddd",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/infrastructure/mysql/typeorm/ormconfig.ts migration:run",
    "migration:show": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/infrastructure/mysql/typeorm/ormconfig.ts migration:show",
    "migration:gen": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/infrastructure/mysql/typeorm/ormconfig.ts migration:generate",
    "migration:revert": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/infrastructure/mysql/typeorm/ormconfig.ts migration:revert",
    "seed": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js --configName src/infrastructure/mysql/typeorm/ormconfig.ts seed"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "dotenv": "^16.0.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.3.5"
  }
}
